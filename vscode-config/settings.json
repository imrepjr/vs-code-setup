{
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)"
  ],
  "auto-rename-tag.activationOnLanguage": [
    "html",
    "xml",
    "javascript",
    "javascriptreact",
    "typescriptreact"
  ],
  "breadcrumbs.enabled": true,
  "breadcrumbs.symbolPath": "off",
  "css.lint.duplicateProperties": "warning",
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true,
    "source.fixAll.eslint": true
  },
  "editor.fontFamily": "'Fira Code Retina', 'Fira Code', 'IBM Plex Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "none",
  "editor.minimap.enabled": false,
  "editor.fontSize": 14,
  "editor.lineHeight": 28,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "[vue]": {
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[graphql]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "[yaml]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.detectIndentation": true,
  "editor.tabSize": 2,
  "editor.rulers": [80, 120],
  "editor.formatOnType": true,
  "editor.tokenColorCustomizations": {
    "[Material Theme Darker High Contrast]": {
      "textMateRules": [
        {
          "scope": [
            "punctuation.definition.comment",
            "comment.block",
            "comment.line",
            "comment.block.documentation"
          ],
          "settings": {
            "foreground": "#ffd000"
          }
        }
      ]
    }
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.suggestSelection": "first",
  "editor.accessibilitySupport": "off",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.syntaxProfiles": {
    "html": {
      "tag_nl": true,
      "inline_break": 1
    }
  },
  "eslint.validate": ["javascript", "javascriptreact"],
  "eslint.alwaysShowStatus": true,
  "materialTheme.accent": "Blue",
  "sync.gist": "b409b981ec3551a6004e8f5ef82779c8",
  "sync.autoUpload": false,
  "sync.autoDownload": false,
  "sync.removeExtensions": false,
  "sync.forceUpload": true,
  "terminal.integrated.fontFamily": "'MesloLGS NF','RobotoMono Nerd Font'",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.cursorBlinking": true,
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "workbench.activityBar.visible": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "One Dark Pro",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#2979FF",
    "list.activeSelectionForeground": "#2979FF",
    "list.inactiveSelectionForeground": "#2979FF",
    "list.highlightForeground": "#2979FF",
    "scrollbarSlider.activeBackground": "#2979FF50",
    "editorSuggestWidget.highlightForeground": "#2979FF",
    "textLink.foreground": "#2979FF",
    "progressBar.background": "#2979FF",
    "pickerGroup.foreground": "#2979FF",
    "tab.activeBorder": "#2979FF",
    "notificationLink.foreground": "#2979FF",
    "editorWidget.resizeBorder": "#2979FF",
    "editorWidget.border": "#2979FF",
    "settings.modifiedItemIndicator": "#2979FF",
    "settings.headerForeground": "#2979FF",
    "panelTitle.activeBorder": "#2979FF",
    "breadcrumb.activeSelectionForeground": "#2979FF",
    "menu.selectionForeground": "#2979FF",
    "menubar.selectionForeground": "#2979FF"
  },
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.tree.indent": 10,
  "workbench.startupEditor": "newUntitledFile",
  "files.eol": "\r\n",
  "diffEditor.ignoreTrimWhitespace": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.useAliasesForRenames": false,
  "cSpell.userWords": ["Grns", "Pressable"],
  "typescript.tsserver.log": "off",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.importModuleSpecifier": "shortest",
  "reactSnippets.settings.importReactOnTop": false,
  "npm-intellisense.importLinebreak": "",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "cSpell.language": "en-GB,en-US",
  "git.path": "/usr/local/bin/git",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.allowBreakpointsEverywhere": true
}
